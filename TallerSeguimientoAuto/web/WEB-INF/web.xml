<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AutoServlet</servlet-name>
        <servlet-class>TallerSeguimientoAuto.Service.AutoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TallerServlet</servlet-name>
        <servlet-class>TallerSeguimientoAuto.Service.TallerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OperarioServlet</servlet-name>
        <servlet-class>TallerSeguimientoAuto.Service.OperarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MantencionServlet</servlet-name>
        <servlet-class>TallerSeguimientoAuto.Service.MantencionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OperacionServlet</servlet-name>
        <servlet-class>TallerSeguimientoAuto.Service.OperacionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>TallerSeguimientoAuto.Service.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteMantencionServlet</servlet-name>
        <servlet-class>TallerSeguimientoAuto.Service.DeleteMantencionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MantencionBuscarServlet</servlet-name>
        <servlet-class>TallerSeguimientoAuto.Service.MantencionBuscarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OperacionBuscarServlet</servlet-name>
        <servlet-class>TallerSeguimientoAuto.Service.OperacionBuscarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteOperacionServlet</servlet-name>
        <servlet-class>TallerSeguimientoAuto.Service.DeleteOperacionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AutoServlet</servlet-name>
        <url-pattern>/AutoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TallerServlet</servlet-name>
        <url-pattern>/TallerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperarioServlet</servlet-name>
        <url-pattern>/OperarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MantencionServlet</servlet-name>
        <url-pattern>/MantencionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperacionServlet</servlet-name>
        <url-pattern>/OperacionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteMantencionServlet</servlet-name>
        <url-pattern>/DeleteMantencionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MantencionBuscarServlet</servlet-name>
        <url-pattern>/MantencionBuscarServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperacionBuscarServlet</servlet-name>
        <url-pattern>/OperacionBuscarServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteOperacionServlet</servlet-name>
        <url-pattern>/DeleteOperacionServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>
            pages/login.jsp
        </welcome-file>
    </welcome-file-list>
</web-app>
